KokkosAlgorithms_C_COMPILER=/usr/bin/mpicc

KokkosAlgorithms_FORTRAN_COMPILER=/usr/bin/mpif90
KokkosAlgorithms_Fortran_COMPILER=/usr/bin/mpif90


## ---------------------------------------------------------------------------
## Compiler flags used by Trilinos/KokkosAlgorithms build
## ---------------------------------------------------------------------------

## Set compiler flags, including those determined by build type
KokkosAlgorithms_CXX_FLAGS= -pedantic -Wall -Wno-long-long -Wwrite-strings  -std=c++11 -O3
KokkosAlgorithms_CXX_COMPILER_FLAGS= -pedantic -Wall -Wno-long-long -Wwrite-strings  -std=c++11 -O3

KokkosAlgorithms_C_FLAGS= -pedantic -Wall -Wno-long-long -std=c99 -O3
KokkosAlgorithms_C_COMPILER_FLAGS= -pedantic -Wall -Wno-long-long -std=c99 -O3

KokkosAlgorithms_FORTRAN_FLAGS= -O3
KokkosAlgorithms_Fortran_COMPILER_FLAGS= -O3

## Extra link flags (e.g., specification of fortran libraries)
KokkosAlgorithms_EXTRA_LD_FLAGS=

## This is the command-line entry used for setting rpaths. In a build
## with static libraries it will be empty. 
KokkosAlgorithms_SHARED_LIB_RPATH_COMMAND=
KokkosAlgorithms_BUILD_SHARED_LIBS=OFF

KokkosAlgorithms_LINKER=/usr/bin/ld
KokkosAlgorithms_AR=/usr/bin/ar

